{
      "purpose": "MedImage Server Configuration File. Example readProxy http://YOURIP:5566/read/yourdir. ",
      "backupTo": null,
      "readProxy": null,
      "listenPort": 5566,
      "httpsKey" : "",
      "httpsCert" : "",
      "allowPhotosLeaving": true,
      "allowGettingRemotePhotos": false,
      "webProxy": null,
      "keepMaster": false,
      "lockDown": true,
      "basicAuthent": "changeme",
      "allowedTypes" : [ { "extension": ".jpg", "mime": "image/jpeg" },
      					 { "extension": ".pdf", "mime": "application/pdf" },
      					 { "extension": ".mp4", "mime": "video/mp4" },
      					 { "extension": ".mp3", "mime": "audio/mpeg" },
      				 	 { "extension": ".m4v", "mime": "video/mp4" },
      				 	 { "extension": ".m4a", "mime": "audio/m4a" },
      				 	 { "extension": ".csv", "mime": "text/csv" },
      				 	 { "extension": ".json", "mime": "application/json" },
      				 	 { "extension": ".heic", "mime": "image/heic" },
      				 	 { "extension": ".heif", "mime": "image/heif" }   ],
      "allowedChars" : "a-zA-z0-9#._-",
      "maxUploadSize" : 16777216,
      "headers" : [ { "header": "Access-Control-Allow-Origin", "value": "*" } ]
}
