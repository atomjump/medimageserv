{
  "author": "AtomJump.com",
  "name": "medimage",
  "description": "An image server for the Android/iPhone app, MedImage.",
  "version": "0.1.6",
  "bin": {
    "medimage": "bin/server.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "start" :"pm2 start medimage.js",
    "stop" : "pm2 stop medimage.js",
    "postinstall" : "ln -s medimage.js bin/server.js; pm2 start medimage.js; pm2 startup"
  },
  "dependencies": {
    "connect": "*",
    "date-format-lite": "*",
    "drivelist": "*",
    "express": "*",
    "fs-extra": "*",
    "mkdirp": "*",
    "multer": "*",
    "multiparty": "*",
    "mv": "*",
    "node-mv": "*",
    "node-uuid": "*",
    "path": "*",
    "request": "*",
    "needle": "*",
    "upath": "*",
    "https": "*"
  },
  "main": "medimage.js",
  "devDependencies": {
    "grunt-contrib-copy": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-jade": "*",
    "grunt": "*"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=4.4"
  }
}
