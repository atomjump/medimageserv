{
  "author": "AtomJump.com",
  "name": "medimage",
  "description": "An image server for the Android/iPhone app, MedImage.",
  "version": "0.1.0",
  "bin": {
    "medimage": "bin/server.js"
  },
  "scripts": {
    "start" :"forever  -l /var/log/medimageserv/forever.log -o /var/log/medimageserv/out.log -e /var/log/medimageserv/err.log -a bin/server.js",
    "stop" : "forever stop bin/server.js",
    "preinstall" : "echo \"Forever being installed...\"; npm install forever -g --unsafe-perm; echo \"Creating logs in /var/log/medimageserv\" mkdir /var/log/medimageserv;",
    "postinstall" : "cp ./newconfig.js ./config.js"
  },
  "dependencies": {
    "connect": "*",
    "date-format-lite": "*",
    "drivelist": "*",
    "express": "*",
    "fs-extra": "*",
    "mkdirp": "*",
    "multer": "*",
    "multiparty": "*",
    "mv": "*",
    "node-mv": "*",
    "node-uuid": "*",
    "path": "*",
    "request": "*",
    "needle": "*",
    "upath": "*",
    "https": "*"
  },
  "main": "bin/server.js",
  "devDependencies": {
    "grunt-contrib-copy": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-jade": "*",
    "grunt": "*"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=4.4"
  }
}
